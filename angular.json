{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "wizdm": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "root": "wizdm",
      "sourceRoot": "wizdm/src",
      "prefix": "wm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/wizdm",
            "index": "wizdm/src/index.html",
            "main": "wizdm/src/main.ts",
            "polyfills": "wizdm/src/polyfills.ts",
            "tsConfig": "wizdm/tsconfig.app.json",
            "assets": [
              "wizdm/src/favicon.ico",
              "wizdm/src/assets",
              {
                "glob": "*.png",
                "input": "./node_modules/emoji-datasource-google/img/google/64/",
                "output": "/assets/emoji/"
              },
              {
                "glob": "20.png",
                "input": "./node_modules/emoji-datasource-google/img/google/sheets-clean/",
                "output": "/assets/emoji/sheets/"
              }
            ],
            "styles": ["wizdm/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["wizdm/src/styles", "elements/src/lib"]
            },
            "scripts": [],
            "fileReplacements": [
              {
                "replace": "wizdm/src/environments/secrets.ts",
                "with": "wizdm/src/environments/secrets.private.ts"
              }
            ],
            "allowedCommonJsDependencies": [
              "firebase/app", "@firebase/app", "@firebase/component", "@firebase/firestore", "@firebase/util",
              "remark-align", "remark-parse", "unified"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "wizdm/src/environments/environment.ts",
                  "with": "wizdm/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wizdm:build",
            "ssl": true,
            "sslCert": "./ssl/server.crt",
            "sslKey": "./ssl/server.key"
          },
          "configurations": {
            "production": {
              "browserTarget": "wizdm:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wizdm:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "wizdm/src/test.ts",
            "polyfills": "wizdm/src/polyfills.ts",
            "tsConfig": "wizdm/tsconfig.spec.json",
            "karmaConfig": "wizdm/karma.conf.js",
            "styles": ["wizdm/src/styles.scss"],
            "scripts": [],
            "assets": ["wizdm/src/favicon.ico", "wizdm/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "wizdm/tsconfig.app.json",
              "wizdm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "wizdm-e2e": {
      "root": "wizdm-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
